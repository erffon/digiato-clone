{
  "name": "digiato-clone",
  "packageManager": "yarn@3.6.0",
  "devDependencies": {
    "autoprefixer": "^10.4.24",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.97.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "scripts": {
    "build:css": "sass src/scss/style.scss temp-style.css && postcss temp-style.css -o dist/css/style.css && rm temp-style.css",
    "watch:css": "sass --watch src/scss/style.scss:dist/css/style.css"
  }
}
